{
	"name": "Google Storage thumbnail viewer",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Display thumbnails for images in Google Storage",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://storage.cloud.google.com/*"],
			"js": ["jquery.js","gstv.js"]
		}
	]
}
